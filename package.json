    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "bower": "^1.7.9",
    "browser-sync": "^2.13.0",
    "bs-fullscreen-message": "^1.1.0",
    "css-loader": "^0.23.1",
    "gulp": "^3.9.1",
    "gulp-hub": "^0.8.0",
